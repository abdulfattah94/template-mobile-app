{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env.development react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android:windows": "SET ENVFILE=.env.development && react-native run-android --variant=devDebug",
    "ios": "react-native run-ios",
    "iphone14": "open -a Simulator && xcrun simctl boot 'iPhone 14'",
    "start": "react-native start  --reset-cache",
    "start-reset": "node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "postinstall": "patch-package",
    "test": "jest",
    "husky:setup": "npx husky-init && npm run husky:commitlint && npm run husky:prettier && npm run husky:lint",
    "husky:commitlint": "npx husky add .husky/commit-msg 'npx --no-install commitlint --edit'",
    "husky:prettier": "npx husky set .husky/pre-commit 'npm run prettier'",
    "husky:lint": "npx husky add .husky/pre-commit 'npm run lint'",
    "clean-up": "rm -rf .git && rm -rf ./assets && git init && npm run husky:setup",
    "clean:android": "cd android && ENVFILE=.env.development ./gradlew clean",
    "debug:android:dev": "cd android && ENVFILE=.env.development ./gradlew app:assembleDevDebug",
    "debug:android:staging": "cd android && ENVFILE=.env.staging ./gradlew app:assembleStagingDebug",
    "debug:android:prod": "cd android && ENVFILE=.env.production ./gradlew app:assembleProdDebug",
    "release:android:dev": "cd android && ENVFILE=.env.development ./gradlew assembleDevRelease",
    "release:android:staging": "cd android && ENVFILE=.env.staging ./gradlew app:assembleStagingRelease",
    "release:android:prod": "cd android && ENVFILE=.env.production ./gradlew app:assembleProdRelease",
    "bundle:dev": "cd android && ENVFILE=.env.development ./gradlew bundleDevRelease",
    "bundle:staging": "cd android && ENVFILE=.env.staging ./gradlew bundleStagingRelease",
    "bundle:prod": "cd android && ENVFILE=.env.production ./gradlew bundleProdRelease",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "cd src && npx prettier --write . && git add .",
    "prepare": "husky install",
    "create:bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res_temp/",
    "codepush:android:dev": "PLATFORM=android DEPLOYMENT=Development BINARY_VERSION=$(grep VERSION_NAME .env.development | cut -d '=' -f2) node scripts/codepush-release.js",
    "codepush:ios:dev": "PLATFORM=ios DEPLOYMENT=Development BINARY_VERSION=$(grep VERSION_NAME .env.development | cut -d '=' -f2) node scripts/codepush-release.js",
    "codepush:android:uat": "PLATFORM=android DEPLOYMENT=Development BINARY_VERSION=$(grep VERSION_NAME .env.staging | cut -d '=' -f2) node scripts/codepush-release.js",
    "codepush:ios:uat": "PLATFORM=ios DEPLOYMENT=Development BINARY_VERSION=$(grep VERSION_NAME .env.staging | cut -d '=' -f2) node scripts/codepush-release.js",
    "windows": "SET ENVFILE=.env.development && react-native run-android --variant=devDebug"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.7",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-firebase/perf": "^18.3.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.32.6",
    "axios": "^1.4.0",
    "jail-monkey": "^2.8.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moti": "^0.26.0",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.8.0",
    "react-native-error-boundary": "^1.2.3",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.6.3",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-haptic-feedback": "^2.0.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.2",
    "react-native-lottie-splash-screen": "^1.1.2",
    "react-native-mmkv": "^2.10.1",
    "react-native-modal": "^13.0.1",
    "react-native-offline": "^6.0.2",
    "react-native-reanimated": "^3.4.2",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0",
    "react-native-svg": "^13.11.0",
    "react-native-vector-icons": "^10.0.0",
    "react-navigation-helpers": "^2.3.1",
    "react-redux": "^8.1.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@redux-saga/types": "^1.2.1",
    "@tsconfig/react-native": "^3.0.0",
    "@types/lodash": "^4.14.196",
    "@types/md5": "^2.3.2",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.7",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-ft-flow": "^2.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-react-prefer-function-component": "^2.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.8.8",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.0.3",
    "typescript": "4.8.4",
    "typescript-styled-plugin": "^0.18.3"
  },
  "engines": {
    "node": ">=16"
  }
}
